{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-restricted-globals": 0,
    "linebreak-style": 0,
    "operator-linebreak": 0,
    "object-curly-newline": 0,
    "prefer-arrow-callback": 0,
    "no-shadow": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "implicit-arrow-linebreak": 0,
    "no-alert": 0,
    "prefer-destructuring": 0,
    "indent": 0
  }
}
